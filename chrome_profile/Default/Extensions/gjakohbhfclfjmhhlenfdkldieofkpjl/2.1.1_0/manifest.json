{
   "action": {
      "default_popup": "popup.html",
      "default_title": "IPRoyal"
   },
   "author": {
      "email": "affiliate@iproyal.com"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "content-scripts/content.js" ],
      "matches": [ "http://localhost/*", "https://*.iproyal.com/*", "https://*.iproyal.dev/*" ]
   }, {
      "all_frames": true,
      "js": [ "content-scripts/share-preferences.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "content-scripts/webrtc-shield.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start",
      "world": "MAIN"
   } ],
   "description": "IPRoyal Proxy Manager allows you to easily control all your proxies in your Browser",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "96": "icon/96.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "126",
   "name": "IPRoyal Proxy Manager",
   "permissions": [ "storage", "proxy", "privacy", "webRequest", "webRequestAuthProvider", "browsingData", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.1"
}
